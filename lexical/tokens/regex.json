{
    "ID":"^([a-zA-Z]|_)+([a-zA-z0-9]|_)*$",
    "CHAR":"^'.'$",
    "INTEGER":"^[0-9]+$",
    "LITERAL":"^\".*\"$",
    "FLOAT":"([0-9]*\\.([0-9]+f?)$)|([0-9]+(\\.|f)$)",
    "ARITHMETIC_OPERATOR":"^((\\+|\\-|\\/|\\*|=){1})$",
    "LOGICAL_OPERATORS": "^((==)|>=|<=|!=|>|<)$",
    "L_PARENTHESIS": "^\\({1}$",
    "R_PARENTHESIS": "^\\){1}$",
    "SEPARATOR":"^,{1}$",
    "END_INSTRUCTION":"^;{1}$",
    "L_BLOCK":"^\\{{1}$",
    "R_BLOCK":"^\\}{1}$",
    "CASE_START":"^:{1}$"
}